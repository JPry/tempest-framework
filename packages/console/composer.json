{
    "name": "tempest/console",
    "description": "The console component provides a way to build commands within the framework or standalone command-line applications.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.5",
        "symfony/filesystem": "^7.3",
        "tempest/core": "3.x-dev",
        "tempest/container": "3.x-dev",
        "tempest/generation": "3.x-dev",
        "tempest/highlight": "^2.11.4",
        "tempest/log": "3.x-dev",
        "tempest/reflection": "3.x-dev",
        "tempest/support": "3.x-dev",
        "tempest/validation": "3.x-dev",
        "ext-readline": "*"
    },
    "suggest": {
        "ext-pcntl": "Required to use some interactive console components.",
        "ext-posix": "Required to use some interactive console components."
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Console\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/tempest",
        "bin/tempest-complete"
    ]
}

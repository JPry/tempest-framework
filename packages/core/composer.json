{
    "name": "tempest/core",
    "description": "The core component of the Tempest framework.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.5",
        "tempest/container": "3.x-dev",
        "tempest/discovery": "3.x-dev",
        "tempest/reflection": "3.x-dev",
        "tempest/support": "3.x-dev",
        "vlucas/phpdotenv": "^5.6.1",
        "symfony/cache": "^7.3",
        "filp/whoops": "^2.15"
    },
    "require-dev": {
        "tempest/validation": "3.x-dev",
        "tempest/intl": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Core\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Core\\Tests\\": "tests"
        }
    }
}
